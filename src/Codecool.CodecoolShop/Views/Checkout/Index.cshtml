@model List<LineItem>

    <div>
        @for (int i = 0; i < Model.Count; i++)
			{
        <table>
            <tr>
                <td>@Model[i].Product.Name</td>
                <td>@Model[i].Quantity</td>
            </tr>
        </table>
			}
    </div>
